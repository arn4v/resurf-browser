{
  "name": "resurf",
  "productName": "Resurf",
  "version": "0.12",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:all": "npm run make:mac-m1 && npm run make:mac-intel",
    "make:mac-m1": "npm run make -- --arch=arm64 --platform=darwin",
    "make:mac-intel": "npm run make -- --arch=x64 --platform=darwin",
    "publish": "electron-forge publish",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,html,css,md,mdx}\"",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "prettier": {
    "tabWidth": 2,
    "arrowParens": "always",
    "semi": false,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-dmg": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
